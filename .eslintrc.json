{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:jasmine/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["jasmine"],
  "rules": {
    "no-use-before-define": "off",
    "camelcase": "off",
    "comma-dangle": "off"
  }
}
